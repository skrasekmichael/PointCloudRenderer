<Application x:Class="PointCloudRenderer.APP.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:converters="clr-namespace:PointCloudRenderer.APP.Converters"
			 xmlns:sys="clr-namespace:System;assembly=mscorlib">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/SceneControlViews.xaml" />
			</ResourceDictionary.MergedDictionaries>

			<sys:Boolean x:Key="True">True</sys:Boolean>
			<sys:Boolean x:Key="False">False</sys:Boolean>

			<converters:EnumTypeToArrayConverter x:Key="EnumType2ArrayConverter"/>
			<converters:RadianAngleToDomainConverter x:Key="RadianAngle2DomainConverter"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
